{
  "AllowedHosts": "*",
  "Cors": {
    "AllowedHosts": [
      "*"
    ],
    "AllowedOrigins": [
      "https://localhost:5443"
    ],
    "AllowedHeaders": [
      "authorization",
      "content-type"
    ],
    "AllowedMethods": [
      "POST"
    ]
  },
  "RabbitMQ": {
    "HostName": "localhost",
    "Port": 5672,
    "UseSsl": false,
    "User": "user",
    "Password": "P@ssw0rd",
    "VirtualHost": "/"
  },
  "Serilog": {
    "Using": [
      "Serilog.Expressions",
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug",
        "Microsoft.AspNetCore.SignalR": "Debug",
        "Microsoft.AspNetCore.Http.Connections": "Debug",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Information"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": "RequestPath like '/health%'"
        }
      },
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": "RequestPath like '%/swagger/%'"
        }
      }
    ],
    "Properties": {
      "HostName": "localhost"
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "Smtp": {
    "From": "admin@domain.com",
    "Host": "localhost",
    "Port": 2525,
    "UseSsl": false,
    "User": "admin",
    "Password": "P@ssw0rd"
  }
}
